{
  "name": "my-portfolio",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.8.3",
    "turbo": "^1.7.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.12.1"
  },
  "packageManager": "pnpm@7.26.1"
}
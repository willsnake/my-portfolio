{
  "name": "@willsnake/my-portfolio-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build && pnpm run tailwind:build",
    "build-storybook": "build-storybook",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "coverage": "vitest run --coverage",
    "dev": "concurrently \"start-storybook -p 6006\" \"npm run tailwind:watch\"",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/tailwind.scss -o assets/app.css",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/tailwind.scss -o assets/app.css -w",
    "test": "vitest",
    "vite:dev": "vite"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-svelte-csf": "^2.0.4",
    "@storybook/addons": "^6.5.9",
    "@storybook/api": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/client-api": "^6.5.9",
    "@storybook/client-logger": "^6.5.9",
    "@storybook/components": "^6.5.9",
    "@storybook/core-common": "^6.5.9",
    "@storybook/core-events": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/svelte": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/react": "^18.0.14",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.2.7",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.16.0",
    "webpack": "^5.73.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ws-components.umd.js",
  "module": "./dist/ws-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/ws-components.es.js",
      "require": "./dist/ws-components.umd.js"
    }
  }
}
